{
  "name": "@wovosoft/wovoui-icons",
  "version": "1.1.7",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:icons  && vite build && npm run generate:types",
    "preview": "vite preview",
    "generate:icons": "node src/generate_icons.js",
    "generate:types": "node src/copyTypes.js"
  },
  "dependencies": {
    "bootstrap-icons": "^1.10.5",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@vitejs/plugin-vue": "^2.3.4",
    "vite": "^2.9.15"
  },
  "files": [
    "dist",
    "src"
  ],
  "typings": "./dist/index.d.ts",
  "description": "Bootstrap Icons in Vue 3",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    "./src/index": "./src/index.ts",
    "./dist/style": "./dist/style.css",
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "author": "Narayan Adhikary",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wovosoft/wovoui-icons.git"
  },
  "keywords": [
    "Bootstrap 5",
    "Icons",
    "Vue 3",
    "wovoui",
    "wovoui-icons"
  ],
  "bugs": {
    "url": "https://github.com/wovosoft/wovoui-icons/issues"
  },
  "homepage": "https://github.com/wovosoft/wovoui-icons#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
